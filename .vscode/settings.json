{
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "idf.portWin": "COM3",
  "idf.flashType": "UART",
  "idf.openOcdConfigs": [
    "interface/jlink.cfg",
    "board/esp32-wrover.cfg"
  ],
  "idf.openOcdLaunchArgs": [
    //"-c program_esp build/blink.bin 0x10000 verify exit"
    "-c set ESP_FLASH_SIZE 0"
  ],
  "idf.openOcdDebugLevel": 2,
  "idf.svdFilePath": "${workspaceFolder}/esp32.svd",
  "idf.adapterTargetName": "esp32",
  "files.associations": {
    "cstdint": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "ostream": "cpp",
    "tuple": "cpp",
    "*.inc": "cpp",
    "complex": "cpp",
    "array": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "gpio_hal.h": "c",
    "initializer_list": "cpp",
    "regex": "cpp",
    "utility": "cpp"
  }
}
